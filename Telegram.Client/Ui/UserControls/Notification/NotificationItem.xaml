<UserControl x:Class="Telegram.Client.Ui.UserControls.Notification.NotificationItem"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Telegram.Client.Ui.UserControls.Notification"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300" d:DataContext="{d:DesignInstance local:NotificationItem}"
             Margin="0 10 0 0"
             MouseDoubleClick="OnMouseDoubleClick"
             Loaded="OnLoaded">
    <StackPanel x:Name="Panel"/>
</UserControl>
